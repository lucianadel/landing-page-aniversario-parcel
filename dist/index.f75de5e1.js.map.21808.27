ace with your target date\r\n\r\n// Function to update countdown timer\r\nfunction updateCountdown() {\r\n    const now = new Date();\r\n    const diff = targetDate.getTime() - now.getTime();\r\n\r\n    const days = Math.floor(diff / (1000 * 60 * 60 * 24));\r\n    const hours = Math.floor((diff % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));\r\n    const minutes = Math.floor((diff % (1000 * 60 * 60)) / (1000 * 60));\r\n    const seconds = Math.floor((diff % (1000 * 60)) / 1000);\r\n\r\n    countdownDays.textContent = days;\r\n    countdownHours.textContent = hours.toString().padStart(2, '0');\r\n    countdownMinutes.textContent = minutes.toString().padStart(2, '0');\r\n    countdownSeconds.textContent = seconds.toString().padStart(2, '0');\r\n\r\n    if (diff <= 0) {\r\n        clearInterval(countdownInterval); // Stop the timer when it reaches 0\r\n        countdownDays.textContent = '00';\r\n        countdownHours.textContent = '00';\r\n        countdownMinutes.textContent = '00';\r\n        countdownSeconds.textContent = '00';\r\n    }\r\n}\r\n\r\n// Start countdown timer\r\nconst countdownInterval = setInterval(updateCountdown, 1000);\r\n\r\n// Function to handle present selection\r\npresentImages.forEach(image => {\r\n    image.addEventListener('click', (event) => {\r\n        const chosenPresentId = event.target.dataset.presentId;\r\n        // Implement logic to store chosenPresentId (e.g., localStorage, server-side)\r\n        presentSelection.classList.remove('hidden'); // Show present confirmation section\r\n    });\r\n});\r\n\r\n// Function to handle confirm button click\r\nconfirmPresentButton.addEventListener('click', () => {\r\n    const userName = document.getElementById('yourName').value;\r\n    if (userName) {\r\n        // Implement logic to send userName and chosenPresentId (e.g., API call, email)\r\n        alert(`Obrigado(a), ${userName}! Seu presente est√° confirmado.`);\r\n        presentSelection.classList.add('hidden'); // Hide present confirmation section\r\n        // Optionally, clear user name field for next selection\r\n    } else {\r\n        alert('Por favor, informe seu nome para confirmar.');\r\n    }\r\n});"],"names":[],"version":3,"file":"index.f75de5e1.js.map","sourceRoot":"/__parcel_source_root/"}